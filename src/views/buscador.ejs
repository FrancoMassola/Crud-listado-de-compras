<%- include ('partials/_header');  %>


<!-- LIST -->
<div class="col-md-8 offset-md-2">
    <table class="table table-bordered table-hover">
        <thead>
            <tr>
                <th>Nro de compra</th>
                <th>Producto</th>
                <th>Descripcion</th>

            </tr>
        </thead>

        <tbody>
            <% for(var i = 0;i < productos_compras.length ; i++){  %>
            <tr>
                <td><%= i + 1 %></td>
                <td><%= productos_compras[i].title %></td>
                <td><%= productos_compras[i].description %></td>

            </tr>
            <% } %>
        </tbody>
    </table>

</div>

</div>
</div>

<%- include ('partials/_footer');  %>